                            {
                                "name": "url",
                                "label": "URL to your page on this social network",
                                "type": "text"
                            },
                            {
                                "name": "icon",
                                "label": "Icon",
                                "type": "select",
                                "options": {
                                    "facebook": "Facebook",
                                    "instagram": "Instagram",
                                    "linkedin": "LinkedIn",
                                    "pinterest": "Pinterest",
                                    "twitter": "Twitter",
                                    "youtube": "YouTube"
                                }
                            }
