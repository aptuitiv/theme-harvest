            {
                "name": "Header",
                "description": "Content for the header that is used on all pages",
                "fields": [
                    {
                        "name": "headerShowSocial",
                        "label": "Show Social",
                        "type": "radio",
                        "options": {
                            "yes": "Yes",
                            "no": "No"
                        },
                        "default": "yes"
                    },
                    {
                        "name": "headerShowPhone",
                        "label": "Show Phone Number",
                        "type": "radio",
                        "options": {
                            "yes": "Yes",
                            "no": "No"
                        },
                        "default": "yes"
                    },
                    {
                        "name": "headerShowAddress",
                        "label": "Show Address",
                        "type": "radio",
                        "options": {
                            "yes": "Yes",
                            "no": "No"
                        },
                        "default": "yes"
                    },
                    {
                        "name": "headerShowPrimaryButton",
                        "label": "Show Primary Button",
                        "type": "radio",
                        "options": {
                            "yes": "Yes",
                            "no": "No"
                        },
                        "default": "yes"
