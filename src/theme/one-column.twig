{% if _page.request.get.new == "yes" %}
    {%  include 'snippets/header-new' %}
{% else %}
    {%  include 'snippets/header' %}
{% endif %}
<div class="Container">
    {% block header %}{% endblock %}
    {% if _page.isHome == false %}
        {{ _page.breadcrumb }}
    {% endif %}
    <div class="Content">
        {{ _page.content('header') }}
        <main id="main">{% block main %}{{ _page.content() }}{% endblock %}</main>
    </div>
</div>
{%  include 'snippets/footer' %}
